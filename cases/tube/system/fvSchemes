FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    // Note: setting coefficient to 1 for the rhoPimpleAdiabaticFoam solver
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;

    div(phi,U)      Gauss upwind;// LUST grad(U);
    div(phi,h)      Gauss upwind;//LUST grad(h);
    div(phi,K)      Gauss upwind;//linear;
    div(phi,rho)    Gauss upwind;//linear;
    div(phiByRho,rho) Gauss upwind;//linear;
    div(phiByRho,U)   Gauss upwind;//linear;
    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;

    div(U)          Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}


// ************************************************************************* //
