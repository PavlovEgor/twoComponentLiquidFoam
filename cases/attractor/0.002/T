FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.002";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    top
    {
        type            calculated;
        value           uniform 300;
    }
    bottom
    {
        type            calculated;
        value           uniform 300;
    }
    left
    {
        type            calculated;
        value           uniform 300;
    }
    right
    {
        type            calculated;
        value           uniform 300;
    }
    frontAndBack
    {
        type            empty;
    }
}


